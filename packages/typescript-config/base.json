{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "target": "ES2022",
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "allowJs": false
  },
  "include": [],
  "files": []
}
